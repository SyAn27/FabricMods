{
  "schemaVersion": 1,
  "id": "expandedstorage",
  "version": "${mod_version}",
  "name": "ExpandedStorage",
  "description": "Adds new storage blocks with varying capacities.",
  "authors": [
    "NinjaPhenix"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/expanded-storage-fabric",
    "sources": "https://github.com/NinjaPhenix/FabricMods/tree/${minecraft_version}",
    "issues": "https://github.com/NinjaPhenix/FabricMods/issues"
  },
  "license": "MIT-0",
  "icon": "assets/expandedstorage/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "ninjaphenix.expandedstorage.common.ExpandedStorage",
      "ninjaphenix.expandedstorage.common.ContainerLibrary::INSTANCE"
    ],
    "client": [
      "ninjaphenix.expandedstorage.client.ExpandedStorageClient::INSTANCE",
      "ninjaphenix.expandedstorage.client.ContainerLibraryClient::INSTANCE"
    ]
  },
  "accessWidener": "expandedstorage.accesswidener",
  "mixins": [
    {
      "config": "expandedstorage.mixins.common.json",
      "environment": "*"
    }
  ],
  "depends": {
    "fabric": ">=${dep_fabric_version}",
    "chainmail": ">=${dep_chainmail_version}"
  }
}