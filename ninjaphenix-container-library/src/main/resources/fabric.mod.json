{
  "schemaVersion": 1,
  "id": "ninjaphenix-container-lib",
  "version": "${mod_version}",
  "name": "NinjaPhenix's Container Library",
  "description": "Library for working with containers.",
  "authors": [
    "NinjaPhenix"
  ],
  "contact": {
    "sources": "https://github.com/NinjaPhenix/FabricMods/tree/${minecraft_version}",
    "issues": "https://github.com/NinjaPhenix/FabricMods/issues"
  },
  "license": "MIT-0",
  "icon": "assets/ninjaphenix-container-lib/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "ninjaphenix.containerlib.ContainerLibrary::INSTANCE"
    ],
    "client": [
      "ninjaphenix.containerlib.impl.client.ContainerLibraryClient::INSTANCE"
    ],
    "rei_plugins_v0": [
      "ninjaphenix.containerlib.impl.client.ContainerLibREIPlugin"
    ]
  },
  "mixins": [
    {
      "config": "ninjaphenix-container-lib.mixins.json",
      "environment": "*"
    }
  ],
  "accessWidener": "ninjaphenix-container-lib.accesswidener",
  "depends": {
    "fabricloader": ">=${dep_loader_version}",
    "fabric": ">=${dep_fabric_version}",
    "minecraft": ">=${dep_minecraft_version}",
    "chainmail": ">=${dep_chainmail_version}"
  },
  "custom": {
    "modmenu:api": true
  }
}